{
  "name": "redux-action-generator",
  "version": "0.0.1",
  "description": "A tool for generating redux action and interface ts code from swagger.",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "src/index.ts",
  "bin": {
    "ts-codegen": "bin/ts-codegen.js"
  },
  "files": [
    "dist",
    "bin",
    "lib",
    "module",
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest",
    "build": "rm -rf dist && tsc",
    "release": "rm -rf lib/ && rm -rf module/ && rollup -c rollup.config.ts",
    "prepare": "npm run build && npm run release && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teobler/redux-action-generator.git"
  },
  "author": "teobler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teobler/redux-action-generator/issues"
  },
  "homepage": "https://github.com/teobler/redux-action-generator#readme",
  "devDependencies": {
    "@babel/core": "7.8.x",
    "@babel/preset-env": "7.8.x",
    "@babel/preset-typescript": "7.8.x",
    "@types/axios": "0.14.x",
    "@types/jest": "24.9.x",
    "@types/lodash": "4.14.x",
    "@types/node": "12.12.x",
    "@types/prettier": "1.19.x",
    "@types/rollup": "0.54.x",
    "axios": "0.19.x",
    "babel-core": "6.26.x",
    "babel-jest": "24.9.x",
    "babel-plugin-pure-calls-annotation": "0.3.x",
    "jest": "24.9.x",
    "lodash": "4.17.x",
    "rollup": "1.32.x",
    "rollup-plugin-babel": "4.4.x",
    "rollup-plugin-typescript": "1.0.1",
    "ts-jest": "25.2.x",
    "ts-node": "8.6.x",
    "tslib": "1.11.x",
    "typescript": "3.7.x"
  },
  "dependencies": {
    "@openapi-integration/openapi-schema": "0.0.6",
    "prettier": "1.19.x",
    "axios": "0.19.x"
  },
  "peerDependencies": {
    "lodash": ">=4.17.15",
    "tslib": ">=1.11.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__types__/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}

{
  "name": "@openapi-integration/swr-request-generator",
  "version": "1.1.0",
  "description": "A tool for generating TypeScript code and interface from swagger by using SWR and axios as client.",
  "bin": {
    "ts-codegen": "bin/ts-codegen.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node src/codegen.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "prepare": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teobler/swr-request-generator"
  },
  "author": "teobler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teobler/swr-request-generator/issues"
  },
  "keywords": [
    "swr",
    "swagger",
    "OpenAPI",
    "API-integration",
    "code generator",
    "interface",
    "TypeScript"
  ],
  "homepage": "https://github.com/teobler/swr-request-generator#readme",
  "devDependencies": {
    "@ts-stack/openapi-spec": "^3.1.5",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "4.14.x",
    "@types/node": "^18.13.0",
    "@types/prettier": "2.7.x",
    "@vitest/coverage-c8": "^0.28.4",
    "ts-node": "10.9.x",
    "typescript": "4.9.x",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "commander": "^10.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "4.17.x",
    "prettier": "2.8.x"
  },
  "peerDependencies": {
    "swr": ">=2.0.0",
    "axios": ">=1.1.3"
  }
}
